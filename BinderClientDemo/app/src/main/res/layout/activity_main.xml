<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >

    <Button
        android:id="@+id/bind_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Bind Service"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/sync_func"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Sync Func"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/oneway_func"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Oneway Func"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/in_out_func"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="out/inout Func"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/send_callback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="客户端回调"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/bind_messenger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Bind Messenger"
        android:textAllCaps="false"
        />

    <Button
        android:id="@+id/call_messenger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Call Messenger"
        android:textAllCaps="false"
        />

</LinearLayout>